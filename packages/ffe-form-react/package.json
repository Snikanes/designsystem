    "jest": "^23.4.2",
    "prop-types": "^15.6.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-motion": "^0.5.2",
    "react-test-renderer": "^16.2.0",
    "sinon": "^7.2.3"
